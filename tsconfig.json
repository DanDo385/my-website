{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/ProjectCards.jsx", "components/WorkCards.jsx", "pages/experience.jsx", "pages/education.jsx", "pages/certifications.jsx", "pages/blog.jsx", "pages/_document.jsx", "pages/_app.tsx", "pages/index.jsx", "pages/portfolio.jsx", "pages/resume.jsx", "components/Carousel.tsx", "components/EducationFormat.jsx", "components/Navbar.jsx", "components/ResumeSelect.tsx"],
  "exclude": ["node_modules"]
}
